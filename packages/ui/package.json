{
  "name": "@zionstate/ui",
  "version": "0.0.1",
  "private": true,
  "source": "src/index.ts",
  "main": "dist/zionstate-ui.cjs.js",
  "module": "dist/zionstate-ui.esm.js",
  "exports": {
    ".": {
      "module": "./dist/zionstate-ui.esm.js",
      "default": "./dist/zionstate-ui.cjs.js"
    },
    "./style": {
      "module": "./style/dist/zionstate-ui-style.esm.js",
      "default": "./style/dist/zionstate-ui-style.cjs.js"
    },
    "./components": {
      "module": "./components/dist/zionstate-ui-components.esm.js",
      "default": "./components/dist/zionstate-ui-components.cjs.js"
    },
    "./classes": {
      "module": "./classes/dist/zionstate-ui-classes.esm.js",
      "default": "./classes/dist/zionstate-ui-classes.cjs.js"
    },
    "./colors": {
      "module": "./colors-entrypoint/dist/zionstate-ui-colors.esm.js",
      "default": "./colors-entrypoint/dist/zionstate-ui-colors.cjs.js"
    },
    "./next": {
      "module": "./next/dist/zionstate-ui-next.esm.js",
      "default": "./next/dist/zionstate-ui-next.cjs.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "style",
    "components",
    "classes",
    "colors-entrypoint",
    "next"
  ],
  "preconstruct": {
    "exports": true,
    "entrypoints": [
      "index.ts",
      "style.ts",
      "components.ts",
      "classes.ts",
      "colors-entrypoint.ts",
      "next.ts"
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "mocha --require ts-node/register ./test/**/*.test.ts",
    "build:pkg": "yarn preconstruct build",
    "build:rollup": "rollup -c ./rollup.config.js",
    "build": "yarn build:pkg && yarn build:rollup ",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "next_dev": "next dev",
    "next_build": "next build",
    "next_prod": "next start",
    "test_of": "cd test && NODE_DEBUG=log ts-node"
  },
