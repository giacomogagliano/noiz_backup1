{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-styled-components", { "ssr": true, "displayName": true }],
    "inline-react-svg"
  ],
  "babelrcRoots": ["*"]
}
